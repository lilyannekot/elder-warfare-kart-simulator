, [enemyHP, ourHP, winCount]);\n\n  const slap = () => {\n    if (haveGone) {\n      alert(\"You've already gone this turn, you must end your turn.\");\n    } else {\n      setEnemyHP(enemyHP - 20);\n      setAnnouncement(\"You slapped them!\");\n    }\n    setHaveGone(true);\n  };\n\n  const nap = () => {\n    if (haveGone) {\n      alert(\"You've already gone this turn, you must end your turn.\");\n    } else {\n      if (ourHP + 30 > 100 + winCount * 5) {\n        let fullhp = 100 + winCount * 5 - ourHP;\n        setHP(ourHP + fullhp);\n        setAnnouncement(\"You healed to full hp!\");\n      } else {\n        setHP(ourHP + 30);\n        setAnnouncement(\"You healed +30hp!\");\n      }\n    }\n    setHaveGone(true);\n  };\n\n  const runOver = () => {\n    if (haveGone) {\n      alert(\"You've already gone this turn, you must end your turn.\");\n    } else {\n      let hitChance = Math.random() * 10;\n      if (hitChance > 0 && hitChance < 2.5) {\n        setAnnouncement(\"OHHHH! You ran them over! -50HP!\");\n        setEnemyHP(enemyHP - 50);\n      } else {\n        console.log(hitChance);\n        setAnnouncement(\"Uh oh, you missed!\");\n      }\n    }\n    setHaveGone(true);\n  };\n\n  const enemyAttack = () => {\n    if (!haveGone) {\n      setAnnouncement(\"Enemy has already gone, it's your turn!\");\n    } else {\n      setHP(ourHP - 20);\n      setHaveGone(false);\n      setAnnouncement(\"You just got slapped! -20hp\");\n    }\n  };\n\n  return (\n    <div className=\"battle-page\">\n      <div className=\"row\">\n        <Wincount wins={playerWins} />\n        <div style={{ display: \"flex\", height: \"500px\" }}>\n          <Hud slap={slap} nap={nap} runover={runOver} />\\\n          <button\n            className=\"text-light\"\n            style={{ height: \"20%\", width: \"20%\", margin: \"0px\" }}\n            onClick={enemyAttack}\n          >\n            End Turn\n          </button>\n          <Player />\n        </div>\n      </div>\n      <div className=\"row\">\n        <div className=\"column\">\n          <HPbar currentHP={ourHP} maxHP={100 + winCount * 5} />\n          <EnemyHPbar\n            enemyCurrentHP={enemyHP}\n            enemyMaxHP={100 + winCount * 5}\n          />\n        </div>\n        <div className=\"column\">\n          <Announcer announcement={announcement} />\n        </div>\n      </div>\n    </div>\n  );\n};\n\nexport default Battle;\n","C:\\Users\\pbark\\Documents\\Projects\\elder-warfare-kart-simulator\\client\\src\\pages\\Loser.js",[],"C:\\Users\\pbark\\Documents\\Projects\\elder-warfare-kart-simulator\\client\\src\\components\\Footer\\index.js",[],["82","83"],"C:\\Users\\pbark\\Documents\\Projects\\elder-warfare-kart-simulator\\client\\src\\components\\Header\\index.js",[],"C:\\Users\\pbark\\Documents\\Projects\\elder-warfare-kart-simulator\\client\\src\\utils\\auth.js",[],"C:\\Users\\pbark\\Documents\\Projects\\elder-warfare-kart-simulator\\client\\src\\utils\\mutations.js",[],"C:\\Users\\pbark\\Documents\\Projects\\elder-warfare-kart-simulator\\client\\src\\components\\BattlePage\\announcer.js",[],"C:\\Users\\pbark\\Documents\\Projects\\elder-warfare-kart-simulator\\client\\src\\components\\BattlePage\\Wincount.js",[],"C:\\Users\\pbark\\Documents\\Projects\\elder-warfare-kart-simulator\\client\\src\\components\\BattlePage\\player.js",[],"C:\\Users\\pbark\\Documents\\Projects\\elder-warfare-kart-simulator\\client\\src\\components\\Hud\\Hud.js",[],"C:\\Users\\pbark\\Documents\\Projects\\elder-warfare-kart-simulator\\client\\src\\components\\BattlePage\\HPbar\\enemyHPbar.js",["84"],"import React from 'react';\n\nconst EnemyHPbar = ({ enemyCurrentHP, enemyMaxHP }) => {\n    \n    const fullWidth = 38;\n\n    const percentHP = enemyCurrentHP/enemyMaxHP;\n    const HPfill = Math.floor(fullWidth * percentHP);\n\n    return (\n        <svg xmlns=\"http://www.w3.org/2000/svg\" width={264} viewBox=\"0 -0.5 44 9\" shape-rendering=\"crispEdges\">\n            <metadata>Made with Pi